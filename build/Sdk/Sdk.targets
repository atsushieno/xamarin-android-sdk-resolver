<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_XamarinAndroidInPackageTargets>$(MSbuildThisFileDirectory)..\build\Xamarin.Android.CSharp.targets</_XamarinAndroidInPackageTargets>
  </PropertyGroup>
  <Import Project="$(_XamarinAndroidInPackageTargets)" 
    Condition="Exists('$(_XamarinAndroidInPackageTargets)')" />
  <Import Project="$(MSBuildExtensionsPath)\Xamarin\Android\Xamarin.Android.CSharp.targets"
    Condition="!Exists('$(_XamarinAndroidInPackageTargets)')" />
</Project>
